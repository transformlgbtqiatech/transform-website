{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "jsxImportSource": "react",
    "paths": {
      "@cms/*": ["./src/keystatic/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@styles/*": ["./src/styles/*"],
      "@store/*": ["./src/store/*"],
      "@astro-collections/*": ["./src/astro-collections/*"],
      "@root/*": ["./*"]
    }
  }
}
